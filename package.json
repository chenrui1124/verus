{
  "name": "verus",
  "version": "0.0.0",
  "type": "module",
  "description": "Vue3 Components.",
  "files": [
    "lib/dist"
  ],
  "keywords": [
    "Vue3",
    "Components"
  ],
  "license": "MIT",
  "main": "./lib/dist/verus.js",
  "exports": {
    ".": "./lib/dist/verus.js",
    "./style": "./lib/dist/style.css"
  },
  "types": "./lib/dist/verus.d.ts",
  "scripts": {
    "dev": "pnpm --filter @verus/vue dev",
    "build": "pnpm --filter @verus/vue build",
    "doc": "pnpm --filter @verus/doc",
    "lib": "pnpm --filter @verus/vue"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-tailwindcss"
    ],
    "tailwindConfig": "./lib/tailwind.config.ts",
    "tailwindAttributes": [
      "class",
      "enterFromClass",
      "enterActiveClass",
      "enterToClass",
      "leaveFromClass",
      "leaveActiveClass",
      "leaveToClass",
      "enter-from-class",
      "enter-active-class",
      "enter-to-class",
      "leave-from-class",
      "leave-active-class",
      "leave-to-class"
    ],
    "bracketSameLine": false,
    "jsxBracketSameLine": true,
    "jsxSingleQuote": true,
    "printWidth": 100,
    "semi": false,
    "singleAttributePerLine": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none",
    "arrowParens": "avoid"
  },
  "dependencies": {
    "mm3": "^0.2.2",
    "@vueuse/core": "^10.7.1",
    "vue": "^3.4.5",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@iconify-json/ri": "^1.1.18",
    "@iconify-json/solar": "^1.1.9",
    "@iconify-json/svg-spinners": "^1.1.2",
    "@iconify/tailwind": "^0.1.4",
    "@types/node": "^20.10.6",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.10",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3",
    "unocss": "^0.58.3",
    "unocss-transformer-alias": "^0.0.7",
    "vite": "^5.0.11",
    "vite-plugin-dts": "^3.7.0",
    "vue-tsc": "^1.8.27"
  }
}
